{
  "version": 2,
  "builds": [
    {
      "src": "api/telegramWebhook.js",
      "use": "@vercel/node"
    },
    {
      "src": "**/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.css",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.js",
      "use": "@vercel/static"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/telegramWebhook",
      "dest": "/api/telegramWebhook.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}