{
  "builds": [
    {
      "src": "api/telegramWebhook.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    },
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/telegramWebhook",
      "dest": "api/telegramWebhook.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
